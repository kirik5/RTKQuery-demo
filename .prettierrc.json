{
  "overrides": [
    {
      "files": ["*.js", "*.jsx"],
      "options": {
        "printWidth": 140,
        "tabWidth": 4,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "avoid",
        "endOfLine": "lf"
      }
    }
  ]
}
